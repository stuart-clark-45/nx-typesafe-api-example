{
  "version": 2,
  "projects": {
    "api-spec": {
      "root": "libs/api-spec",
      "sourceRoot": "libs/api-spec/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/api-spec",
            "tsConfig": "libs/api-spec/tsconfig.lib.json",
            "packageJson": "libs/api-spec/package.json",
            "main": "libs/api-spec/src/index.ts",
            "assets": ["libs/api-spec/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api-spec/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api-spec"],
          "options": {
            "jestConfig": "libs/api-spec/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
