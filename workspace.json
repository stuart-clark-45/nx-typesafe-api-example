{
  "version": 2,
  "projects": {
    "api-spec": {
      "root": "libs/api-spec",
      "sourceRoot": "libs/api-spec/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/js:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/api-spec",
            "tsConfig": "libs/api-spec/tsconfig.lib.json",
            "packageJson": "libs/api-spec/package.json",
            "main": "libs/api-spec/src/index.ts",
            "assets": ["libs/api-spec/*.md"]
          }
        },
        "typesafe-open-api": {
          "executor": "nx:run-commands",
          "options": {
            "command": "npx tsx node_modules/@typesafe-api/open-api/src/cli.js --tsConfigFile libs/api-spec/tsconfig.lib.json --output libs/api-spec/openapi.yml --title nx-typesafe-api-example --routes 'libs/api-spec/src/routes/**/*.ts'"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api-spec/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api-spec"],
          "options": {
            "jestConfig": "libs/api-spec/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "cdk-lambda": {
      "root": "apps/cdk-lambda",
      "projectType": "application",
      "sourceRoot": "apps/cdk-lambda/src",
      "targets": {
        "deploy": {
          "executor": "@ago-dev/nx-aws-cdk-v2:deploy",
          "options": {}
        },
        "destroy": {
          "executor": "@ago-dev/nx-aws-cdk-v2:destroy",
          "options": {}
        },
        "bootstrap": {
          "executor": "@ago-dev/nx-aws-cdk-v2:bootstrap",
          "options": {}
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/cdk-lambda/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/cdk-lambda"],
          "options": {
            "jestConfig": "apps/cdk-lambda/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "express": {
      "root": "apps/express",
      "sourceRoot": "apps/express/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/express",
            "main": "apps/express/src/main.ts",
            "tsConfig": "apps/express/tsconfig.app.json",
            "assets": ["apps/express/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/express/src/environments/environment.ts",
                  "with": "apps/express/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:node",
          "options": {
            "buildTarget": "express:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/express/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/express"],
          "options": {
            "jestConfig": "apps/express/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "frontend": {
      "root": "apps/frontend",
      "sourceRoot": "apps/frontend/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/frontend",
            "index": "apps/frontend/src/index.html",
            "main": "apps/frontend/src/main.tsx",
            "polyfills": "apps/frontend/src/polyfills.ts",
            "tsConfig": "apps/frontend/tsconfig.app.json",
            "assets": [
              "apps/frontend/src/favicon.ico",
              "apps/frontend/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/frontend/src/environments/environment.ts",
                  "with": "apps/frontend/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "frontend:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "frontend:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/frontend/**/*.{ts,tsx,js,jsx}"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/frontend"],
          "options": {
            "jestConfig": "apps/frontend/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "frontend-e2e": {
      "root": "apps/frontend-e2e",
      "sourceRoot": "apps/frontend-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/frontend-e2e/cypress.json",
            "tsConfig": "apps/frontend-e2e/tsconfig.e2e.json",
            "devServerTarget": "frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "frontend:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/frontend-e2e/**/*.{js,ts}"]
          },
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": [],
      "implicitDependencies": ["frontend"]
    },
    "serverless": {
      "root": "apps/serverless",
      "sourceRoot": "apps/serverless/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/serverless",
            "main": "apps/serverless/src/main.ts",
            "tsConfig": "apps/serverless/tsconfig.app.json",
            "assets": ["apps/serverless/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/serverless/src/environments/environment.ts",
                  "with": "apps/serverless/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx sls offline --httpPort 7810",
            "cwd": "apps/serverless"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/serverless/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/serverless"],
          "options": {
            "jestConfig": "apps/serverless/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    }
  },
  "$schema": "./node_modules/nx/schemas/workspace-schema.json"
}
